    <class>entity.Module</class>
    <class>entity.Sessionlog</class>
    <class>entity.Role</class>
    <class>entity.Employeestatus</class>
    <class>entity.Designation</class>
    <class>entity.Category</class>
    <class>entity.Userrole</class>
    <class>entity.User</class>
    <class>entity.Privilage</class>
    <class>entity.Itempackagestatus</class>
    <class>entity.Itempackagetype</class>
    <class>entity.Operationlog</class>
    <class>entity.Itempackage</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/twoelephantsfireworks?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="1234"/>
    </properties>
  </persistence-unit>
</persistence>
